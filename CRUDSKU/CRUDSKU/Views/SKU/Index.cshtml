@model List<SKU>

@{
	ViewData["Title"] = "SKUs";
}

<table class="table">
	<thead>
		<tr>
			<th>Codigo</th>
			<th>Nombre</th>
			<th>Descripcion</th>
			<th>Editar</th>
			<th>Eliminar</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Codigo</td>
				<td>@item.Nombre</td>
				<td>@item.Descripcion</td>
				<td><button class="btn btn-primary"><i class="bi bi-trash3"></i></button></td>
				<td><button class="btn btn-danger">eliminar</button></td>
			</tr>
		}
	</tbody>
</table>
